{
    "files.eol": "\n",
    "editor.formatOnSave": true,
    "cSpell.customDictionaries": {
        "custom-dictionary-convdate": {
            "name": "custom-dictionary-convdate",
            "path": "${workspaceFolder:convdate}/.cspell/custom-dictionary-convdate.txt",
            "addWords": true,
            "scope": "workspace"
        },
        "custom-dictionary-rust": {
            "name": "custom-dictionary-rust",
            "path": "${workspaceFolder:convdate}/.cspell/custom-dictionary-rust.txt",
            "addWords": true,
            "scope": "workspace"
        }
    },
    "cSpell.enableFiletypes": [
        "markdown",
        "rust"
    ]
}